{
  "name": "vite-example",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --write src/**/*{.tsx,.ts,.css}",
    "lint-staged": "lint-staged",
    "stylelint": "stylelint src/**/*.css",
    "cy:open": "cypress open --e2e --browser chrome",
    "cy:run": "cypress run --e2e --browser chrome",
    "e2e-open": "start-server-and-test 'npm run dev' http://localhost:3000 cy:open-e2e",
    "e2e-run": "start-server-and-test 'npm run dev' http://localhost:3000 cy:run-e2e"

  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.39",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.10.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "prettier": "3.2.5",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^16.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-istanbul": "^6.0.2"
  },
  "eslintConfig": {
    "extends": "./.eslintrc.json"
  },
  "lint-staged": {
    "src/**/*{tsx,ts}": [
      "npm run lint",
      "npm run prettier",
      "git add"
    ],
    "*.css": [
      "npm run stylelint",
      "npm run prettier",
      "git add"
    ]
  }
}
